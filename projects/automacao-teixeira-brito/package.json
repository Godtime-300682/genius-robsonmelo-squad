{
  "name": "automacao-teixeira-brito",
  "version": "1.0.0",
  "description": "Sistema de Automação IA 24/7 - Teixeira Brito Advocacia",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
